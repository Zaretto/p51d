   <name>stall</name>
   <mode>looped</mode>
   <path>Sounds/stall.wav</path>
   <property>/sim/alarms/stall-warning</property>
   <volume>
    <property>/sim/alarms/stall-warning</property>
    <max>1.0</max>
   </volume>
  </stall>-->
  
  <guns>
    <name>gunfire</name>
    <mode>looped</mode>
    <path>Aircraft/p51d/Sounds/gunfire_1.wav</path>
    <condition>
       <property>controls/armament/gun-trigger</property> 
       <!--<property>ai/submodels/trigger</property> -->
       <property>ai/submodels/submodel[1]/count</property>
       <property>/controls/armament/gunsight/power-on</property>
    </condition>
    <volume>
       <factor>1.2</factor>
       <max>1.0</max>
    </volume>
  </guns>
  
  <rockets>
    <name>rocket sound</name>
    <mode>once</mode>
    <path>Aircraft/p51d/Sounds/rockets.wav</path>
    <property>/fdm/jsbsim/systems/armament/rocket-sound</property>
    <volume>
       <factor>1.2</factor>
       <max>1.0</max>
    </volume>
  </rockets>

  <!--                  Toggle Switches                  -->

      <click>
        <name>guns and gun sight on</name>
        <path>Sounds/click.wav</path>
        <condition>
             <property>/controls/armament/guns-enabled</property>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>gun sight off</name>
        <path>Sounds/click.wav</path>
        <condition>
          <not>
             <property>/controls/armament/gunsight/power-on</property>
          </not>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>guns and gun sight on</name>
        <path>Sounds/click.wav</path>
        <condition>
             <property>/controls/armament/gunsight/power-on</property>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click> 

      <click>
        <name>gun sight and camera on</name>
        <path>Sounds/click.wav</path>
        <condition>
          <less-than>
             <property>/controls/armament/gunsight/power-on</property>
             <value>0</value>
          </less-than>
        </condition>
        <condition>
          <not>
             <property>/controls/armament/guns-enabled</property>
          </not>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>Battery on</name>
        <path>Sounds/click.wav</path>
        <condition>
          <property>controls/engines/engine/master-bat</property>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>
      <click>
        <name>Battery off</name>
        <path>Sounds/click.wav</path>
         <condition>
          <not>
            <property>controls/engines/engine/master-bat</property>
          </not>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>
   
      <click>
        <name>Gen on</name>
        <path>Sounds/click.wav</path>
        <condition>
          <property>controls/engines/engine/master-alt</property>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>
      <click>
        <name>Gen off</name>
        <path>Sounds/click.wav</path>
         <condition>
          <not>
            <property>controls/engines/engine/master-alt</property>
          </not>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

     <click>
        <name>Delay off</name>
        <path>Sounds/click.wav</path>
        <condition>          
          <property>/controls/armament/instant</property>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>
      
      <click>
        <name>Delay on</name>
        <path>Sounds/click.wav</path>
         <condition>
          <not>
            <property>/controls/armament/instant</property>
          </not>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>right chem arm</name>
        <path>Sounds/click.wav</path>
        <condition>
           <less-than>
             <property>/controls/armament/right-bomb-arm-chem-release</property>
             <value>0</value>
           </less-than>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>right bomb arm</name>
        <path>Sounds/click.wav</path>
        <condition>
           <greater-than>
             <property>/controls/armament/right-bomb-arm-chem-release</property>
             <value>0</value>
           </greater-than>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>right bomb arm off</name>
        <path>Sounds/click.wav</path>
        <condition>
          <not>
             <property>/controls/armament/right-bomb-arm-chem-release</property>
          </not>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>left bomb arm</name>
        <path>Sounds/click.wav</path>
        <condition>
           <less-than>
             <property>/controls/armament/left-bomb-arm-chem-release</property>
             <value>0</value>
           </less-than>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>left bomb chem arm</name>
        <path>Sounds/click.wav</path>
        <condition>
           <greater-than>
             <property>/controls/armament/left-bomb-arm-chem-release</property>
             <value>0</value>
           </greater-than>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>left bomb arm off</name>
        <path>Sounds/click.wav</path>
        <condition>
          <not>
             <property>/controls/armament/left-bomb-arm-chem-release</property>
          </not>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>weapons select bombs</name>
        <path>Sounds/click.wav</path>
        <condition>
           <greater-than>
             <property>/controls/armament/weapons-select</property>
             <value>0</value>
           </greater-than>
        </condition>
        <condition>
           <less-than>
             <property>/controls/armament/weapons-select</property>
             <value>3</value>
           </less-than>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>weapons select rockets</name>
        <path>Sounds/click.wav</path>
        <condition>
           <greater-than>
             <property>/controls/armament/weapons-select</property>
             <value>2</value>
           </greater-than>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>weapons select guns</name>
        <path>Sounds/click.wav</path>
        <condition>
          <not>
             <property>/controls/armament/weapons-select</property>
          </not>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click> 
 
      <click>
        <name>mag off</name>
        <path>Sounds/click.wav</path>
        <condition>
           <not>
             <property>/controls/engines/engine/magnetos</property>
           </not>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>mag 1</name>
        <path>Sounds/click.wav</path>
        <condition>
             <and>
              <greater-than>
                <property>/controls/engines/engine/magnetos</property>
                <value>0</value>
              </greater-than>
              <less-than>
                <property>/controls/engines/engine/magnetos</property>
                <value>2</value>
              </less-than>
           </and>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>mag 2</name>
        <path>Sounds/click.wav</path>
        <condition>
           <and>
              <greater-than>
                <property>/controls/engines/engine/magnetos</property>
                <value>1</value>
              </greater-than>
              <less-than>
                <property>/controls/engines/engine/magnetos</property>
                <value>3</value>
              </less-than>
           </and>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>mag 3</name>
        <path>Sounds/click.wav</path>
        <condition>
           <greater-than>
             <property>/controls/engines/engine/magnetos</property>
             <value>2</value>
           </greater-than>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>tank 0</name>
        <path>Sounds/click.wav</path>
        <condition>
           <not>
             <property>/controls/fuel/tank-selector</property>
           </not>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>tank 1</name>
        <path>Sounds/click.wav</path>
        <condition>
             <property>/controls/fuel/tank-selector</property>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>tank 2</name>
        <path>Sounds/click.wav</path>
        <condition>
           <equeals>
             <property>/controls/fuel/tank-selector</property>
             <value>2</value>
           </equeals>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>tank 3</name>
        <path>Sounds/click.wav</path>
        <condition>
           <equeals>
             <property>/controls/fuel/tank-selector</property>
             <value>3</value>
           </equeals>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>tank 4</name>
        <path>Sounds/click.wav</path>
        <condition>
           <equeals>
             <property>/controls/fuel/tank-selector</property>
             <value>4</value>
           </equeals>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>fuel on</name>
        <path>Sounds/click.wav</path>
        <condition>
             <property>/controls/fuel/on</property>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

      <click>
        <name>fuel off</name>
        <path>Sounds/click.wav</path>
        <condition>
           <not>
             <property>/controls/fuel/on</property>
           </not>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

</fx>
 
</PropertyList>
