<?xml version="1.0"?>
<PropertyList>

 <path>elevatorTrimWheel.ac</path>

 <animation>
  <type>knob</type>
  <drag-direction>vertical</drag-direction>
  <drag-scale-px>10.0</drag-scale-px>
  <object-name>elevatorTrimWheel</object-name>
  <property>/controls/flight/elevator-trim</property>
  <factor>1440</factor>
  <axis>
   <y>-1</y>
  </axis>
         <action>
             <binding>
                 <command>property-adjust</command>
                 <property>/controls/flight/elevator-trim</property>
                 <factor>0.002</factor>
                 <min>-1</min>
                 <max>1</max>
                 <wrap>0</wrap>
             </binding>
         </action>
         <hovered>
             <binding>
                 <command>set-tooltip</command>
                 <tooltip-id>elevator-trim</tooltip-id>
                 <label>Elevator trim: %.3f</label>
                 <property>/controls/flight/elevator-trim</property>
             </binding>
         </hovered>
     </animation>

 <animation>
  <type>rotate</type>
  <object-name>Needle</object-name>
  <property>/controls/flight/elevator-trim</property>
  <interpolation>
   <entry>
    <ind>-1.0</ind>
    <dep>-50</dep>
   </entry>
   <entry>
    <ind>-0.0</ind>
    <dep>20</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>50</dep>
   </entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>


