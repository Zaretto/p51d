                <tooltip-id>wing-pos-lgt-switch</tooltip-id>
                <label>Position lights wings: %s</label>
                <property>/controls/lighting/wing-position-lights</property>
                <mapping>nasal</mapping>
                <script>
                    var modes = ['OFF', 'BRIGHT', 'DIM'];
                    return modes[arg[0]];
                </script>
            </binding>
        </hovered>
    </animation>

