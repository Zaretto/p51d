<?xml version="1.0"?>

<PropertyList>

    <path>electrical-panel.ac</path>

    <animation>
        <type>translate</type>
        <object-name>rec-lights-key</object-name>
        <property>/controls/lighting/rec-light-key</property>
        <interpolation>
        <entry>
            <ind>0.0</ind>
            <dep>0</dep>
        </entry>
        <entry>
            <ind>1.0</ind>
            <dep>-0.003</dep>
        </entry>
        </interpolation>
        <axis>
            <z>1</z>
        </axis>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>rec-lights-key</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>/controls/lighting/rec-light-key</property>
                <value type="int">1</value>
            </binding>
            <mod-up>
                <binding>
                    <command>property-assign</command>
                    <property>/controls/lighting/rec-light-key</property>
                    <value type="int">0</value>
                </binding>
            </mod-up>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>rec-lights-key</tooltip-id>
                <label>Click to enable receiver light</label>
            </binding>
        </hovered>
    </animation>

    <animation>
