<?xml version="1.0"?>

<PropertyList>

    <path>p51d-jw-vsi.ac</path>

    <animation>
        <type>material</type>
        <object-name>face</object-name>
        <object-name>needle</object-name>
        <emission>
            <factor-prop>/controls/lighting/instruments-norm</factor-prop>
            <red>0.35</red>
            <green>0.35</green>
            <blue>0.4</blue>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>bezel</object-name>
        <emission>
            <factor-prop>/controls/lighting/cabin-norm</factor-prop>
            <red>0.2</red>
            <green>0.1</green>
            <blue>0.1</blue>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>needle</object-name>
        <property>/instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
        <interpolation>
            <entry>
                <ind>-6000</ind>
                <dep>262</dep>
            </entry>
            <entry>
                <ind>-2000</ind>
                <dep>172</dep>
            </entry>
            <entry>
                <ind>-1000</ind>
                <dep>136</dep>
            </entry>
            <entry>
                <ind>-500</ind>
                <dep>113</dep>
            </entry>
            <entry>
                <ind>0</ind>
                <dep>90</dep>
            </entry>
            <entry>
                <ind>500</ind>
                <dep>67</dep>
            </entry>
            <entry>
                <ind>1000</ind>
                <dep>44</dep>
            </entry>
            <entry>
                <ind>2000</ind>
                <dep>8</dep>
            </entry>
            <entry>
                <ind>6000</ind>
                <dep>-82</dep>
            </entry>
        </interpolation>
        <axis>
            <x>1</x>
        </axis>
    </animation>

</PropertyList>
