<?xml version="1.0"?>
<PropertyList>

 <path>p51d-jw-tach.ac</path>

 <animation>
  <type>material</type>
  <object-name>face</object-name>
  <object-name>needle</object-name>
  <emission>
    <factor-prop>controls/lighting/instruments-norm</factor-prop>
    <red>0.35</red>
    <green>0.35</green>
    <blue>0.4</blue>
  </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>bezel</object-name>
  <emission>
    <factor-prop>/controls/lighting/cabin-norm</factor-prop>
    <red>0.2</red>
    <green>0.1</green>
    <blue>0.1</blue>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>needle</object-name>
  <property>engines/engine/rpm</property>
  <min>0</min>
  <max>4500</max>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>-160</dep>
   </entry>
   <entry>
    <ind>4500</ind>
    <dep>160</dep>
   </entry>
  </interpolation>
  <axis>
   <x>-1</x>
  </axis>
 </animation>

</PropertyList>


