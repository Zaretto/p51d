<?xml version="1.0"?>

<PropertyList>

    <path>p51d-jw-asi.ac</path>

    <animation>
        <type>material</type>
        <object-name>face</object-name>
        <object-name>needle</object-name>
        <emission>
            <factor-prop>/controls/lighting/instruments-norm</factor-prop>
            <red>0.35</red>
            <green>0.35</green>
            <blue>0.4</blue>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>bezel</object-name>
        <emission>
            <factor-prop>/controls/lighting/cabin-norm</factor-prop>
            <red>0.2</red>
            <green>0.1</green>
            <blue>0.1</blue>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>needle</object-name>
        <property>/instrumentation/airspeed-indicator/ias-mph</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>50</ind>
                <dep>14</dep>
            </entry>
            <entry>
                <ind>100</ind>
                <dep>42</dep>
            </entry>
            <entry>
                <ind>300</ind>
                <dep>231</dep>
            </entry>
            <entry>
                <ind>700</ind>
                <dep>341</dep>
            </entry>
        </interpolation>
        <axis>
            <x>-1</x>
        </axis>
    </animation>

</PropertyList>
