<?xml version="1.0"?>
<PropertyList>

 <path>p51d-jw-acc.ac</path>

 <animation>
  <type>material</type>
  <object-name>face</object-name>
  <object-name>needle</object-name>
  <emission>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    <red>0.35</red>
    <green>0.35</green>
    <blue>0.4</blue>
  </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>bezel</object-name>
  <emission>
    <factor-prop>/controls/lighting/cabin-norm</factor-prop>
    <red>0.2</red>
    <green>0.1</green>
    <blue>0.1</blue>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>needle</object-name>
  <property>/accelerations/pilot/z-accel-fps_sec[0]</property>
  <interpolation>
   <entry>
    <ind>161</ind>
    <dep>-90</dep>
   </entry>
   <entry>
    <ind>-386</ind>
    <dep>240</dep>
   </entry>
  </interpolation>
  <axis>
   <x>-1</x>
  </axis>
 </animation>

</PropertyList>
