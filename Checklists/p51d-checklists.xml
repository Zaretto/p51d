                <value>BOTH</value>
            </item>
            <item include="fuel-boost-on.xml">
                <name>Fuel Boost</name>
                <value>ON</value>
            </item>
            <item include="primer-on.xml">
                <name>Primer (3 or 4 seconds when cold, 1 second when hot)</name>
                <value>ON</value>
            </item>
            <item include="mixture-run.xml">
                <name>Mixture (in principle after engine start. Set it anyway)</name>
                <value>RUN</value>
            </item>
            <item include="primer-off.xml">
                <name>Primer</name>
                <value>OFF</value>
