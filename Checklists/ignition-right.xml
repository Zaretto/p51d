<?xml version="1.0"?>

<!-- Magneto right check -->
<PropertyList>
    <condition>
        <and>
            <equals>
                <property>/controls/engines/engine/magnetos</property>
                <value>1</value>
            </equals>
            <greater-than>
                <property>/engines/engine/rpm</property>
                <value alias="../../../../min-rpm"/>
            </greater-than>
        </and>
    </condition>
    <marker>
        <x-m>2.6</x-m>
        <y-m>-0.0941</y-m>
        <z-m>0.0</z-m>
        <scale>3</scale>
    </marker>
</PropertyList>
